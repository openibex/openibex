  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Initialize",
      "runtimeExecutable": "npx",
      "args": [
        "tsx",
        "packages/cli/src/index.ts",
        "init"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "USDC Connect with startblock",
      "runtimeExecutable": "npx",
      "args": [
        "tsx",
        "packages/cli/src/index.ts",
        "start",
        "--connect",
        "eip155:1/erc20:0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48",
        "--block",
        "608246"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "USDC scrape with usd-circle",
      "runtimeExecutable": "npx",
      "args": [
        "tsx",
        "packages/cli/src/index.ts",
        "start",
        "--scrape",
        "usd-circle"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "USDC check balance",
      "runtimeExecutable": "npx",
      "args": [
        "tsx",
        "packages/cli/src/index.ts",
        "exec",
        "--contract",
        "eip155:1/erc20:0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48",
        "--function",
        "balanceOf",
        "--args",
        "0x6d4cc96bd9135c25cbcaa4d38a0b514798a60360"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
